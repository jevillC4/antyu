  "tokenColors": [
    {
      "scope": "source",
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#626a73"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": ["string.regexp", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#5cdbd3"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffd666"
      }
    },
    {
      "name": "Built-in constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#ffd666"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "Member Variable",
      "scope": ["variable.member"],
      "settings": {
        "foreground": "#69c0ff"
      }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#69c0ff"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#a0ff58"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#a0ff58"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.definition.string",
        "punctuation.definition.array",
        "punctuation.definition.entity",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.separator.inheritance",
        "comment.line.keyword.punctuation.yard",
        "comment.line.punctuation.yard",
        "punctuation.section.function.ruby",
        "punctuation.separator.object.ruby"
      ],
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Punctuation 2",
      "scope": [
        "keyword.other.unit.percentage",
        "keyword.other.unit.px",
        "keyword.other.unit.rem",
        "keyword.other.unit.em"
      ],
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Accessor",
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#ff9c6e"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
